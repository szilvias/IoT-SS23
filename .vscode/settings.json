{
    "cmake.configureOnOpen": false,
    "files.associations": {
        "sstream": "c"
    },
    
    // The following items should be replaced throughout this file 
    // with paths that match your environment. These will depend on
    // where you have cloned the esp-idf source repo as well as where
    // you chose to install the IDF tools. An easy way to do this is
    // to highlight the item and use Ctrl-D to highlight all instances.
    // You can then replace them all at once with your path.

 
    // For more information about tool installation see
    // https://docs.espressif.com/projects/esp-idf/en/latest/esp32/get-started/index.html#step-3-set-up-the-tools


    // The port and buad rate for flashing and monitoring your device
    //"esp_device_port": "/dev/cu.usbserial-0215E868",
    "esp_device_port": "/dev/cu.usbserial-0215EAEE",
    "esp_device_baud": 921600,

     // The tools needed for building, flashing, and debugging
    // ONLY change your espressif tools path and esp-idf repo
    // location in the paths below
    "idfInstFolder": "/Users/gerndt/.espressif",

    "idfFolder": "/Users/gerndt/esp/esp-idf",
    
    "esp_python_bin": "${config:idfInstFolder}/python_env/idf4.4_py3.9_env/bin",

    "esp_idf_command": "${config:esp_python_bin}/python ${config:idfFolder}/tools/idf.py",

    "esp_elf_bin": "${config:idfInstFolder}/tools/xtensa-esp32-elf/esp-2021r2-patch3-8.4.0/xtensa-esp32-elf/bin",

    "esp_elf_binutils": "${config:idfInstFolder}/tools/esp32ulp-elf/2.28.51-esp-20191205/esp32ulp-elf-binutils/bin",

    "openocd_root_dir": "${config:idfInstFolder}/tools/openocd-esp32/v0.11.0-esp32-20211220/openocd-esp32",

    "esp_openocd_bin": "${config:idfInstFolder}/tools/openocd-esp32/v0.11.0-esp32-20211220/openocd-esp32/bin",

    "esp_extra_paths": "/bin:/usr/local/bin:${config:esp_python_bin}:/usr/bin:${config:esp_elf_bin}:${config:esp_elf_binutils}:${config:esp_openocd_bin}",

    "esp_gdb": "${esp_elf_bin}/xtensa-esp32-elf-gdb",
    "git.ignoreLimitWarning": true,

}